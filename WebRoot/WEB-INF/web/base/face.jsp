<%@ page pageEncoding="UTF-8"%>
<div class = "facebox face-switch" id = "facebox">
	<div class="face-switch" style="float:left;width:100%;height:19px;margin-top:3px;border-radius:0px;border-bottom:1px solid #d1d1d1;">
		<span id = "yct_face" class = "face_type face_type_checked">洋葱头</span>
		<span id = "al_face" class = "face_type">阿狸</span>
		<span id = "bobo_face" class = "face_type">BOBO</span>
