	<div id = "yct_face_block" class = "face_block" style="float:left;width:100%;margin-top:2px;">
	</div>
	<div id = "al_face_block" class = "face_block" style="float:left;width:100%;margin-top:2px;display:none;">
	</div>
	<div id = "bobo_face_block" class = "face_block" style="float:left;width:100%;margin-top:2px;display:none;">
	</div>
	<div id="face_preview_box" style="position:absolute;top:20px;right:-52px;width:50px;height:50px;border:1px solid #e68303;border-left:0px;background:white;display:none;border-radius:0px 3px 3px 0px">
