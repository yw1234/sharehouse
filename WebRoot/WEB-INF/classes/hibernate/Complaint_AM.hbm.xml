<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name = "model.Complaint_AM" table = "complaint_am">
		<id name = "id" type = "long">
			<generator class="native"></generator>
		</id>
		<property name="senderid" type = "long" not-null = "true" >
		</property>
		<property name="complainted_userid" type = "long">
		</property>
		<property name="reason" type = "string">
		</property>
		<property name="content" type = "string">
		</property>
		<property name="addtime" type = "java.util.Date" not-null = "true"></property>
	</class>
</hibernate-mapping>