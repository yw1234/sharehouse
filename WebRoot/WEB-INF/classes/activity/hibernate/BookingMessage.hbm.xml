<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name = "activity.sellbook.model.BookingMessage" table = "activity_sellbook_bookingMessage">
		<id name = "id" type = "long">
			<generator class="native"></generator>
		</id>
		<property name="userid" type = "long" not-null = "true"></property>
		<property name="bookid" type = "long" not-null = "true">
		</property>
		<property name="count" type = "integer" not-null = "true"></property>
		<property name="content" type = "string"></property>
		<property name="bookingType" type = "string" not-null = "true"></property>
		<property name="getTime" type = "string"></property>
		<property name="bookingTime" type = "java.util.Date" not-null = "true"></property>
		<property name="campus" type = "string"></property>
		<property name="dorm" type = "string" ></property>
		<property name="dormNumber" type = "string"></property>
		<property name="bookerName" type = "string" ></property>
		<property name="bookerContact" type = "string"></property>
		<property name="isComplete" type = "string" length = "1"></property>
		<property name="totalPrice" type = "integer"></property>
	</class>
</hibernate-mapping>