<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name = "activity.lostfound.model.Lost" table = "activity_lostfound_lost">
		<id name = "id" type = "long">
			<generator class="native"></generator>
		</id>
		<property name="userid" type = "long" not-null = "true"></property>
		<property name="place" type = "string" not-null = "true">
		</property>
		<property name="name" type = "string" not-null = "true"></property>
		<property name="content" type = "string"></property>
		<property name="campus" type = "string"></property>
		<property name="add_time" type = "java.util.Date" not-null = "true"></property>
		<property name="contact" type = "string"></property>
		<property name="place_gene" type = "string"></property>
	</class>
</hibernate-mapping>