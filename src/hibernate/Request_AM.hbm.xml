<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name = "model.Request_AM" table = "request_am">
		<id name = "id" type = "long">
			<generator class="native"></generator>
		</id>
		<property name="senderid" type = "long">
		</property>
		<property name="req_type" type = "string" >
		</property>
		<property name="content" type = "string">
		</property>
		<property name="req_content" type = "string">
		</property>
		<property name="remark_id" type = "long">
		</property>
		<property name="addtime" type = "java.util.Date" not-null = "true"></property>
	</class>
</hibernate-mapping>