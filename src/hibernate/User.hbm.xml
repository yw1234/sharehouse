  <property generated="never" lazy="false" name="privacy_pShowUser" type="string"/>
  <property generated="never" lazy="false" name="privacy_pShowCircle" type="string"/>
  <property generated="never" lazy="false" length="1" name="isNewUser" type="string"/>
  <property generated="never" lazy="false" length="1"
   name="registerByAPI" type="string"/>
  <property generated="never" lazy="false" length="50" name="hs_year" type="string"/>
  <property length="50" name="educational" type="string"/>
  <property length="200" name="dorm" type="string"/>
  <property length="100" name="campus" type="string"/>
 </class>
</hibernate-mapping>
