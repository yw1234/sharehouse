   not-null="true" type="string"/>
  <property generated="never" lazy="false" name="phone" type="string"/>
  <property generated="never" lazy="false" name="province" type="string"/>
  <property generated="never" lazy="false" name="city" type="string"/>
  <property generated="never" lazy="false" name="school" not-null="true" type="string"/>
  <property generated="never" lazy="false" name="department" type="string"/>
  <property generated="never" lazy="false" name="qq" type="string"/>
  <property generated="never" lazy="false" name="open_id" type="string"/>
  <property generated="never" lazy="false" name="type" not-null="true" type="string"/>
  <property generated="never" lazy="false" name="head_ico"
   not-null="true" type="string"/>
  <property generated="never" lazy="false" name="head_ico_big" type="string"/>
  <property generated="never" lazy="false" name="freeze" not-null="true" type="string"/>
  <property generated="never" lazy="false" name="visted_times"
   not-null="true" type="integer"/>
  <property generated="never" lazy="false" name="add_time"
   not-null="true" type="java.util.Date"/>
  <property generated="never" lazy="false" name="register_ip"
   not-null="true" type="string"/>
  <property generated="never" lazy="false" name="lastlogin_ip" type="string"/>
