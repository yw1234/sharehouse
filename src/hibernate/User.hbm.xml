   not-null="true" type="integer"/>
  <property generated="never" lazy="false" name="level" not-null="true" type="integer"/>
  <property generated="never" lazy="false" name="friend_number"
   not-null="true" type="integer"/>
  <property generated="never" lazy="false" name="circle_number"
   not-null="true" type="integer"/>
  <property generated="never" lazy="false" name="lastlogin_time"
   not-null="true" type="java.util.Date"/>
  <property generated="never" lazy="false" name="continue_online"
   not-null="true" type="integer"/>
  <property generated="never" lazy="false" name="login_active" type="integer"/>
  <property generated="never" lazy="false" name="send_active" type="integer"/>
  <property generated="never" lazy="false" name="lastsendtime" type="java.util.Date"/>
