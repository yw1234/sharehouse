<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name = "model.Picture" table = "picture">
		<id name = "id" type = "long">
			<generator class="native"></generator>
		</id>
		<property name="typeid" type = "long" not-null = "true" >
		</property>
		<property name="type" type = "string" not-null = "true" >
		</property>
		<property name="url" type = "string" not-null = "true" >
		</property>
		<property name="thumbs_url" type = "string">
		</property>
		<property name="tiny_url" type = "string">
		</property>
		<property name="is_shared" type = "string" length="1">
		</property>
	</class>
</hibernate-mapping>